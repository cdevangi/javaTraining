
public class Demo2D {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int [][] matrix = new int[3][3];
		
		for(int i=0;i<3;i++)
		{
			for(int j=0;i<3;j++) {
				matrix[i][j] = i+j+1;
			}
		}
		
		for(int x[]:matrix) {
			for(int y:x) {
				System.out.print(y+"\t");
			}
			System.out.println();
		}
			
			
	}

}
